{
    CLASS clase1{
        SHORT _sc1,
        VOID fc1(SHORT pfc1){
            _sc1 = pfc1+10_s,
        },
    },

    CLASS clase2{
        clase1,
        SHORT _sc2,
        VOID fc1(SHORT pfc1){
            _sc1 = pfc1,
        },
    },

    clase1 c1,
    c1._sc1 = 1_s,

    clase2 c2,
    c2._sc2 = 30_s,

    SHORT pr,
    pr = 10_s,

    c2.clase1.fc1(pr),
}