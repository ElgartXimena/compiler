{
    CLASS clase1{
        SHORT att1,
        VOID metodo1(){
            SHORT m1,
        },
    },

    CLASS clase2{
        SHORT att2,
        VOID metodo2(){
            SHORT m2,
        },
        clase1,
    },

    CLASS clase3{
        SHORT att3,
        VOID metodo3(),
        clase2,
    },

    clase3 c3,
    c3.metodo3(),

    IMPL FOR clase3 : {
        VOID metodo3(){
            SHORT m3,
        },
    },

    c3.metodo3(),
}